<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nayan.androidchart">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.nayan.androidchart.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nayan.androidchart.BarChartActivity" />
        <activity android:name="com.nayan.androidchart.PIChartActivity" />
        <activity android:name="com.nayan.androidchart.BarGraph2Activity" />
        <activity android:name="com.nayan.androidchart.AreaChartActivity" />
        <activity android:name="com.nayan.androidchart.LineChartActivity" />
        <activity android:name="com.nayan.androidchart.MultiLineChartActivity" />
    </application>

</manifest>